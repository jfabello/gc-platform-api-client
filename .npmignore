# Ignore the .DS_Store files generated by macOS
.DS_Store

# Ignore the .env environment variables file
.env

# Ignore the node_modules folder and package-lock.json file generated by npm
/node_modules/
package-lock.json

# Ignore scripts for setting the environment variables
set-env-*.sh

# Ignore the Genesys Cloud public API swagger file
publicapi-v2-latest.json

# Ignore the scratchpad.js files
scratchpad.js

# Ignore the test coverage reports generated by Jest
/coverage/

# Ignore the test files
/tests/
*.test.js
jest.config.js

# Ignore the ESLint configuration files
eslint.config.js